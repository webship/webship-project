{
  "name": "webship/webship-project",
  "description": "Project template for webship projects with composer",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://github.com/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/webship",
    "source": "http://cgit.drupalcode.org/webship"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org"
    },
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "composer/installers": "~1.0 || ~2.0",
    "oomphinc/composer-installers-extender": "~1.0 || ~2.0",
    "cweagans/composer-patches": "~1.0",
    "drupal/core-composer-scaffold": "~9.0",
    "webship/webship": "9.0.x-dev"
  },
  "require-dev": {
    "drupal/core-dev": "~9.0",
    "drush/drush": "~10.0",
    "drupal/webtest": "~9.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false,
    "process-timeout": 1800,
    "github-protocols": [
      "https,ssh"
    ]
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    }
  }
}